{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mml",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_token"
      }
    },
    "_token": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "note"
        },
        {
          "type": "SYMBOL",
          "name": "rest"
        },
        {
          "type": "SYMBOL",
          "name": "length_command"
        },
        {
          "type": "SYMBOL",
          "name": "octave_command"
        },
        {
          "type": "SYMBOL",
          "name": "octave_up"
        },
        {
          "type": "SYMBOL",
          "name": "octave_down"
        },
        {
          "type": "SYMBOL",
          "name": "instrument_command"
        },
        {
          "type": "SYMBOL",
          "name": "chord"
        },
        {
          "type": "SYMBOL",
          "name": "track_separator"
        }
      ]
    },
    "note": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "pitch",
          "content": {
            "type": "SYMBOL",
            "name": "note_pitch"
          }
        },
        {
          "type": "FIELD",
          "name": "accidentals",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "accidental"
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "duration",
              "content": {
                "type": "SYMBOL",
                "name": "duration"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "dots",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "dot"
            }
          }
        }
      ]
    },
    "note_pitch": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "c"
        },
        {
          "type": "STRING",
          "value": "d"
        },
        {
          "type": "STRING",
          "value": "e"
        },
        {
          "type": "STRING",
          "value": "f"
        },
        {
          "type": "STRING",
          "value": "g"
        },
        {
          "type": "STRING",
          "value": "a"
        },
        {
          "type": "STRING",
          "value": "b"
        }
      ]
    },
    "accidental": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "STRING",
          "value": "-"
        }
      ]
    },
    "dot": {
      "type": "STRING",
      "value": "."
    },
    "duration": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "rest": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "r"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "duration",
              "content": {
                "type": "SYMBOL",
                "name": "duration"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "dots",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "dot"
            }
          }
        }
      ]
    },
    "length_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "l"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "duration"
          }
        }
      ]
    },
    "octave_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "o"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "duration"
          }
        }
      ]
    },
    "octave_up": {
      "type": "STRING",
      "value": "<"
    },
    "octave_down": {
      "type": "STRING",
      "value": ">"
    },
    "instrument_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "instrument_name"
          }
        }
      ]
    },
    "instrument_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "PATTERN",
          "value": "[A-Za-z][A-Za-z0-9]*"
        }
      ]
    },
    "chord": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "FIELD",
          "name": "notes",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "chord_note"
            }
          }
        },
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "duration",
              "content": {
                "type": "SYMBOL",
                "name": "duration"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "dots",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "dot"
            }
          }
        }
      ]
    },
    "chord_note": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "pitch",
          "content": {
            "type": "SYMBOL",
            "name": "note_pitch"
          }
        },
        {
          "type": "FIELD",
          "name": "accidentals",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "accidental"
            }
          }
        }
      ]
    },
    "track_separator": {
      "type": "STRING",
      "value": ";"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}