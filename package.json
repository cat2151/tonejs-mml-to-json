{
  "name": "tonejs-mml-to-json",
  "version": "0.1.0",
  "description": "MML to JSON converter for tonejs-json-sequencer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./pkg/*": "./pkg/*"
  },
  "files": [
    "dist",
    "pkg",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:wasm": "cd rust && wasm-pack build --target web --out-dir ../pkg",
    "build:ts": "tsc",
    "build:libs": "node scripts/copy-libs.js",
    "build": "npm run build:wasm && npm run build:ts && npm run build:libs",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:wasm": "node test/wasm-test.mjs && node test/wasm-integration-test.mjs && node test/demo-test.mjs && node test/wasm-init-test.mjs",
    "dev": "pnpm run test:watch",
    "test:summary": "node .github/scripts/test-summary.cjs",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "mml",
    "tonejs",
    "music",
    "parser",
    "rust",
    "wasm"
  ],
  "author": "cat2151",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "http-server": "^14.1.1",
    "tonejs-json-sequencer": "github:cat2151/tonejs-json-sequencer",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^22.0.0",
    "dotenv": "^17.2.0",
    "web-tree-sitter": "^0.26.3"
  }
}
